{
  "name": "therapy-sessions",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "test": "npm run test --prefix server",
    "format": "prettier --write ./",
    "e2e": "cd e2e && pytest",
    "migrate": "npm run migrate:up --prefix server"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "prettier": "^3.4.2"
  }
}
